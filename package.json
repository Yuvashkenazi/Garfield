{
  "name": "garfield",
  "version": "1.0.0",
  "description": "",
  "author": "Yuval Ashkenazi",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=21.5.0"
  },
  "scripts": {
    "build": "babel src --out-dir dist --extensions .ts",
    "start": "npm run build && node --enable-source-maps dist/index.js",
    "docker:build:dev": "docker build --platform linux/amd64 -t yuvashkenazi/garfield .",
    "docker:build:prod": "docker build --platform linux/arm64/v8 -t yuvashkenazi/garfield .",
    "docker:run": "docker run -d --restart unless-stopped -v garfield-data:/usr/src/garfield-data --name garfield yuvashkenazi/garfield"
  },
  "exports": [
    "./dist/index.js"
  ],
  "dependencies": {
    "@types/node": "^20.12.10",
    "@types/uuid": "^9.0.7",
    "cheerio": "^1.0.0-rc.12",
    "discord.js": "^14.15.3",
    "giphy-api": "^2.0.2",
    "moment-timezone": "^0.5.45",
    "nconf": "^0.12.1",
    "node-fetch": "^3.3.2",
    "openai": "^4.52.4",
    "sequelize": "^6.37.3",
    "sqlite3": "5.0.2",
    "systeminformation": "^5.21.24",
    "unofficial-jisho-api": "^2.3.4",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.23.3",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^7.16.0",
    "babel-loader": "^9.1.2",
    "eslint": "^8.56.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  }
}
